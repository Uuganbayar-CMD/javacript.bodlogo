var sum = 0;
var limit = 1000;
for(var i=2; i<=limit; i++){
  if(anhniiToo(i)===true)console.log(i+'anhnii too mun');
}
console.log (anhniiToo());

function anhniiToo(too){
  var anhniiToo=true;
  for(var x=2; x<too; x++){
    if(too % x ===0){
      anhniiToo=false;
      break;
      
    }
  }
  return anhniiToo;
}
